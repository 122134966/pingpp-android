<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android"> 

    <style name="aipay_prompt_dialog" parent="@android:style/Theme.Dialog">
		<item name="android:windowFrame">@null</item>    
　　 	    <item name="android:windowNoTitle">true</item>   
		<item name="android:textColor">@color/aipay_text_black</item>
		<item name="android:windowBackground">@color/aipay_transparent</item>
    	<item name="android:windowIsFloating">true</item>
    </style>
    <style name="aipay_prompt_login_dialog" parent="@android:style/Theme.Dialog">
		<item name="android:windowFrame">@null</item>
　　 		<item name="android:windowNoTitle">true</item>   
		<item name="android:textColor">#ff000000</item>
		<item name="android:windowBackground">@color/aipay_transparent</item>
    	<item name="android:windowIsFloating">true</item>
		<item name="android:background">@color/aipay_transparent</item>
    </style>
    <style name="aipay_Theme_Activity_Base" parent="@android:style/Theme.Translucent.NoTitleBar">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:textColor">@color/aipay_black</item>
        <item name="android:textSize">15sp</item>
    </style>
    <style name="aipay_Theme_Activit" parent="aipay_Theme_Activity_Base">
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowBackground">@color/aipay_gray</item>
    </style></resources>